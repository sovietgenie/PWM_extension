{
  "manifest_version": 2,
  "name": "PWM Extension BETA",
  "version": "0.1",
    
    
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.2.1.min.js", "content.js"]
    }
  ],
    
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
  },
    
  "permissions": [
        "activeTab",
        "storage",
      "background"
  ],
    "background": {
    "scripts": ["jquery-3.2.1.min.js"]
  }
   
}